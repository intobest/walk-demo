<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="UserDemoSQL">

	<!-- 查询用户列表-->
	<select id="queryUserList" resultType="org.walkframework.data.util.DataMap">
		SELECT A.*
		  FROM TD_M_USER A
		 WHERE A.USER_STATE = '1'
	</select>
</mapper>